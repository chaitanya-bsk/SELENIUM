		driverHelper = new WebdriverHelper();
			driverHelper.startChromeInstance(DriverMode.HEADED);
			driverHelper.navigateToPage("https://login");
			driverHelper.waitForPageToLoad();
			Thread.sleep(2000);
			//driverHelper.setCookies("ergd", "hgf");
			//driverHelper.clickIfElementExists("btn-default");
			String loginbutton = "css=[class*='login-container'] [class*='form-action'] button";
			driverHelper.waitForElementToAppear(loginbutton);
			driverHelper.clickElement(loginbutton);
			String email= "css=[class*='new-idform'] [class*='email']";
			driverHelper.typeOnElement(email,"bsk.chaitanya93@gmail.com");
			String next="css=[class*='mbr_signin'] [class*='one-btn-container'] button";
			driverHelper.waitForElementToAppear(next);
			driverHelper.clickElement(next);
			String password= "css=[type='password']";
			driverHelper.typeOnElement(password,"samsung@135");
			String signin="css=[class*='mbr_signin'] [class*='one-btn-container'] button";
			driverHelper.waitForElementToAppear(signin);
			driverHelper.clickElement(signin);
			String notnow="css=[class*='under-content'] [class*='one-btn-container'] button";
			driverHelper.waitForElementToAppear(notnow);
			driverHelper.clickElement(notnow);
